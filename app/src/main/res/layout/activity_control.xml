            android:id="@+id/recognizeBtn"
            android:layout_width="300px"
            android:layout_height="300px"
            android:text="慧眼识物"/>

        <Button
            android:id="@+id/fastBtn"
            android:layout_width="300px"
            android:layout_height="300px"
            android:layout_marginLeft="400px"
            android:text="快充"/>

        <Button
            android:id="@+id/videoBtn"
            android:layout_width="300px"
            android:layout_height="300px"
            android:layout_marginLeft="800px"
            android:text="随行翻译"/>

        <Button
            android:id="@+id/dressUpBtn"
            android:layout_width="300px"
            android:layout_height="300px"
            android:layout_marginLeft="1200px"
            android:text="ID换装"/>

    </FrameLayout>
