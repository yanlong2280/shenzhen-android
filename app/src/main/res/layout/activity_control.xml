    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/recognizeF"
        android:background="#444">
        <Button
            android:id="@+id/recognizeFBack"
            android:layout_width="300px"
            android:layout_height="300px"
            android:text="返回"/>
    </FrameLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/fastF"
        android:background="#772">
        <Button
            android:id="@+id/fastFBack"
            android:layout_width="300px"
            android:layout_height="600px"
            android:text="返回"/>

    </FrameLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/fullScreenF"
        android:background="#444">

        <Button
            android:id="@+id/fullScreenFBack"
            android:layout_width="300px"
            android:layout_height="300px"
            android:text="返回"/>
        <Button
            android:id="@+id/game"
            android:layout_width="300px"
            android:layout_height="300px"
            android:layout_marginLeft="400px"
            android:text="找不同"/>

    </FrameLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/selectF"
        android:background="#772">
        <Button
            android:id="@+id/selectFBack"
            android:layout_width="300px"
            android:layout_height="600px"
            android:text="返回"/>

    </FrameLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/shootF"
        android:background="#444">

        <Button
            android:id="@+id/shootFBack"
            android:layout_width="300px"
            android:layout_height="300px"
            android:text="返回"/>

    </FrameLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/callF"
        android:background="#444">

        <Button
            android:id="@+id/callFBack"
            android:layout_width="300px"
            android:layout_height="300px"
            android:text="返回"/>
    </FrameLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/videoF"
        android:background="#555">
        <SurfaceView
            android:id="@+id/sv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />
    </FrameLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/dressUpF"
        android:background="#666">
        <Button
            android:id="@+id/dressUpFBack"
            android:layout_width="300px"
            android:layout_height="300px"
            android:text="返回"/>
    </FrameLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/nameplateF"
        android:background="@mipmap/mate10">

        <ImageView
            android:id="@+id/bj1"
            android:layout_width="994px"
            android:layout_height="187px"
            android:layout_gravity="top"
            android:layout_marginLeft="550px"
            android:background="@mipmap/mate10_3"/>
        <ImageView
            android:id="@+id/bj2"
            android:layout_width="500dp"
            android:layout_height="400dp"
            android:layout_gravity="bottom|left"
            android:background="@mipmap/mate10_1"/>
        <ImageView
            android:id="@+id/bj3"
            android:layout_width="500dp"
            android:layout_height="400dp"
            android:layout_gravity="bottom|right"
            android:background="@mipmap/mate10_2"/>

        <ImageView
            android:id="@+id/bjBtn"
            android:layout_width="300dp"
            android:layout_height="200dp"
            android:layout_gravity="bottom|left"
            android:visibility="invisible"
            android:background="@mipmap/honor_v10"/>
    </FrameLayout>

